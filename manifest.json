{
	"manifest_version": 2,
	"name":"Dommage pour toi Kevin",
	"description": "See internet like nobody",
	"version": "1.0",
	"browser_action":
	{
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
	{
		"matches": ["http://www.google.com/*"],
		"css": ["css/style.css"],
		"js": ["js/jquery-2.2.1.min.js", "js/popup.js", "js/firebase.js"]
	}]
}
